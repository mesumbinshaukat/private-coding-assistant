{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/**": {
      "excludeFiles": [
        ".next/**",
        "*.cache/**",
        "node_modules/**",
        "public/**",
        "app/**",
        "desktop-app/**",
        "scripts/**",
        "tests/**",
        "examples/**",
        "docs/**",
        "*.md",
        "*.txt",
        "*.log",
        "*.pyc",
        "__pycache__/**",
        ".git/**",
        ".github/**",
        ".vscode/**",
        ".idea/**",
        "venv/**",
        "env/**",
        ".env*",
        "*.bat",
        "*.sh",
        "*.ps1",
        "models/**",
        "training/**",
        "memory/**",
        "logs/**",
        "requirements_full.txt",
        "requirements_phases.json",
        "install_dependencies.py",
        "api/agent_core.py",
        "api/dependency_manager.py",
        "api/index_full.py",
        "api/index_vercel.py",
        "api/requirements_backup.txt",
        "api/requirements_full.txt",
        "api/requirements_minimal.txt",
        "api/utils/**"
      ]
    }
  }
}
